<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.crudcadastro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Cadastro"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AgendaTelefonica">

        <activity android:name="com.example.crudcadastro.DadosPessoaisActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.crudcadastro.MainActivity"/>
        </activity>
        <activity android:name="com.example.crudcadastro.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>